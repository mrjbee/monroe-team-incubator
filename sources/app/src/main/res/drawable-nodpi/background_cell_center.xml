<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="@color/background.main.light"/>
        </shape>
    </item>
    <item>
        <inset
            android:insetTop="1px"
            android:insetBottom="1px"
            android:insetLeft="1px"
            android:insetRight="1px">
            <shape>
                <solid android:color="@color/background.white" />
            </shape>
        </inset>
    </item>
</layer-list>